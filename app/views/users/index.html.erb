
<!-- <%= @users.inspect %> -->

<%= will_paginate @users %>
<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Mobile</th>
  </tr>
  <% @users.each do | i | %>
    <tr>
      <td><%= image_tag i.image.photo(:thumb) if i.image.present? %></td>
      <td><%= i.name %></td>
      <td><%= i.email %></td>
      <td><%= i.mobile_no %></td>
      <td><%= link_to "Edit", edit_user_path(i)%></td>
      <td><%= link_to "Show", user_path(i)%></td>
      <td><%= link_to "Delete", i, method: "delete", data: {confirm: "Are you sure?"}%></td>
    </tr>
  <%end%>
</table>
